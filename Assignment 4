Question 1.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<=5;i++)
	{
		printf("MySirG\n");
	}
}

Question 2.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<11;i++)
	printf("%d\n",i);
}

Question 3.
#include<stdio.h>
int main()
{
	int i;
	for(i=10;i>0;i--)
	printf("%d\n",i);
}

Question 4.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<21;i++)
	{
		if(i&1)
		{
			printf("%d\n",i);
		}
	}
}

