Question 6.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<21;i++)
	{
		if(i%2==0)
		{
			printf("%d\n",i);
		}
	}
}

Question 7.
#include<stdio.h>
int main()
{
	int i;
	for(i=20;i>0;i--)
	{
		if(i%2==0)
		{
			printf("%d\n",i);
		}
	}
}

Question 8.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<21;i++)
	{
		if(i%2==0)
		{
			printf("square of %d is : %d\n",i,i*i);
		}
	}
}

Question 9.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<11;i++)
	printf("cube of %d is:%d\n",i,i*i*i);
}

Question 10.
#include<stdio.h>
int main()
{
	int i;
	for(i=1;i<11;i++)
	printf("Table of 5*%d:%d\n",i,5*i);
}
